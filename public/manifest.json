{
  "name": "getpass",
  "version": "0.13.0",
  "manifest_version": 2,
  "description": "stateless cross-platform password manager (generator)",
  "permissions": [
    "storage",
    "webNavigation",
    "tabs",
    "*://*/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "512": "logo.png"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "getpass",
    "default_popup": "index.html"
  }
}